| 22 => (Tokens.LBRACK(yypos, yypos + 1))
| 24 => (Tokens.RBRACK(yypos, yypos + 1))
| 26 => (Tokens.LBRACE(yypos, yypos + 1))
| 28 => (Tokens.RBRACE(yypos, yypos + 1))
| 3 => (linePos := yypos :: !linePos; continue())
