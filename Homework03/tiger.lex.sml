| 101 => (Tokens.NIL(yypos, yypos + 3))
| 105 => (Tokens.FOR(yypos, yypos + 3))
| 111 => (Tokens.WHILE(yypos, yypos + 5))
| 116 => (Tokens.ELSE(yypos, yypos + 4))
| 12 => (linePos := yypos :: !linePos; 
		    commentDepth := !commentDepth + 1; 
